{
  "manifest_version": 2,
  "name": "Lorem Ipsum Generator",
  "description": "Populate lorem ipsum with lightening speed",
  "version": "1.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "http://*/*",
    "https://*/*",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["dist/inject.js"]
    }
  ]
}